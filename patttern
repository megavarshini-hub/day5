#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   

    
        int rows;
        scanf("%d", &rows);
        for(int a=1;a<=rows;a++)
        {
        for(int b=1;b<=rows;b++)
            
{
    printf("*");
}
            {
                
             printf("\n");
            }
            }
    return 0;
}
